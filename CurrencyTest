public class CurrencyConverterTest {
  private WebDriver driver;
  private Map<String, Object> vars;
  JavascriptExecutor js;
  @Before
  public void setUp() throws MalformedURLException {
    driver = new RemoteWebDriver(new URL("http://localhost:4444/wd/hub"), DesiredCapabilities.chrome());
    js = (JavascriptExecutor) driver;
    vars = new HashMap<String, Object>();
  }
  @After
  public void tearDown() {
    driver.quit();
  }
  @Test
  public void currencyConverter() {
    driver.get("http://52.9.229.212:8443/CurrencyConverter/");
    driver.manage().window().setSize(new Dimension(1296, 696));
    driver.findElement(By.id("convertFrom")).click();
    driver.findElement(By.id("convertFrom")).click();
    driver.findElement(By.id("convertFrom")).click();
    {
      WebElement dropdown = driver.findElement(By.id("convertFrom"));
      dropdown.findElement(By.xpath("//option[. = 'Japanese Yen (JPY)']")).click();
    }
    driver.findElement(By.id("convertFrom")).click();
    driver.findElement(By.id("convertFrom")).click();
    {
      WebElement dropdown = driver.findElement(By.id("convertFrom"));
      dropdown.findElement(By.xpath("//option[. = 'US Dollar (USD)']")).click();
    }
    driver.findElement(By.id("convertFrom")).click();
    driver.findElement(By.id("convertFrom")).click();
    {
      WebElement dropdown = driver.findElement(By.id("convertFrom"));
      dropdown.findElement(By.xpath("//option[. = 'Indian Rupee (INR)']")).click();
    }
    driver.findElement(By.id("convertFrom")).click();
    driver.findElement(By.id("convertFrom")).click();
    {
      WebElement dropdown = driver.findElement(By.id("convertFrom"));
      dropdown.findElement(By.xpath("//option[. = 'Pound sterling (GBP)']")).click();
    }
    driver.findElement(By.id("convertFrom")).click();
    driver.findElement(By.cssSelector(".form-inline > .ng-pristine")).click();
    driver.findElement(By.cssSelector(".form-inline > .ng-untouched")).sendKeys("1000");
  }
}